start: abstract_term

abstract_term: atom_term -> atom_term
  | fname "(" abstract_term_list ")" -> composed_abstract_term

logic_simple: abstract_term | "~" abstract_term | 
alternative: abstract_term ("or" abstract_term)

abstract_term_list: [abstract_term ("," abstract_term)*]

fname: CNAME
atom_term: CNAME

